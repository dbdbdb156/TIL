using namespace std;
#define _CRT_SECURE_NO_WARNINGS

#include <cstdio>

int n;
int a[51];
bool visited[51]; 
bool stop = false;

void printList(int arr[],int index,int depth) {
	int i;
	if (depth == n - 1) { 
		for (i = 0; i < n-1; ++i)
			printf("%d ", arr[i]); 
		if (i == n - 1) printf("%d\n", arr[i]);
		stop = true;
		return; 
	}
	if (index == -1) {
		for (i = 0; i < n; ++i)
		{
			visited[i] = true;
			arr[0] = a[i];
			printList(arr,i, 0);
			visited[i] = false;
		}
		return;
	}

	for(i=0;i<n;++i)
		if (a[index] + 1 != a[i] && i != index && !visited[i] && !stop)
		{
			visited[i] = true;
			arr[depth + 1] = a[i];
			printList(arr,i,depth+1);
			visited[i] = false;
		}
	
}

int main(void) {

	int arr[100];
	int i;
	scanf("%d", &n);
	for (i = 0; i < n; ++i) {
		scanf("%d", &a[i]);
		visited[i] = false;
	}

	printList(arr,-1,-1);
	
	return 0;
}
